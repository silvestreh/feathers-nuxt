{
  "name": "feathers-nuxt",
  "description": "A Feathers + Nuxt demo/boilerplate",
  "version": "0.0.0",
  "homepage": "https://github.com/silvestreh/feathers-nuxt",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Silvestre Herrera",
    "email": "silvestre.herrera@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "coverage": "cross-env NODE_ENV=test nyc npm run mocha",
    "test": "npm run eslint && npm run coverage",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "cross-env DEBUG=nuxt:* nodemon src/",
    "start": "npm run build && node src/",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.16",
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^1.2.9",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.9",
    "@feathersjs/socketio-client": "^1.2.1",
    "compression": "^1.7.4",
    "cookie-storage": "^5.0.3",
    "cors": "^2.8.5",
    "feathers-nedb": "^5.0.1",
    "feathers-vuex": "^1.7.0",
    "helmet": "^3.20.0",
    "nedb": "^1.8.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.9.1",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^2.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
